{
  "/item/scan/{barcode}": {
    "get": {
      "x-mojo-to": "Com::InteractiveInventory::Controllers::Items#scanItem",
      "operationId": "scanItem",
      "tags": ["inventory"],
      "produces": ["application/json"],
      "parameters": [
        {
          "name": "barcode",
          "in": "path",
          "required": true,
          "type": "string",
          "description": "The barcode of the item to scan"
        }
      ],
      "responses": {
        "200": {
          "description": "Item data retrieved successfully",
          "schema": {
            "type": "object",
            "additionalProperties": true
          }
        },
        "400": {
          "description": "Missing barcode parameter"
        },
        "404": {
          "description": "Item not found"
        },
        "500": {
          "description": "Internal server error"
        }
      }
    }
  },
  "/item/field": {
    "post": {
      "x-mojo-to": "Com::InteractiveInventory::Controllers::Items#modifyItemField",
      "operationId": "modifyItemField",
      "tags": ["inventory"],
      "produces": ["application/json"],
      "consumes": ["application/json"],
      "parameters": [
        {
          "name": "body",
          "in": "body",
          "required": true,
          "description": "Single item field modification data",
          "schema": {
            "type": "object",
            "required": ["barcode", "fields"],
            "properties": {
              "barcode": {
                "type": "string",
                "description": "The barcode of the item to modify"
              },
              "fields": {
                "type": "object",
                "additionalProperties": {
                  "type": "string"
                },
                "description": "The fields to update with their new values"
              }
            }
          }
        }
      ],
      "responses": {
        "200": {
          "description": "Item updated successfully",
          "schema": {
            "type": "object",
            "properties": {
              "barcode": {
                "type": "string"
              },
              "status": {
                "type": "integer"
              },
              "success": {
                "type": "string"
              }
            }
          }
        },
        "400": {
          "description": "Missing barcode or fields to update"
        },
        "404": {
          "description": "Item not found"
        }
      }
    }
  },
  "/item/fields": {
    "post": {
      "x-mojo-to": "Com::InteractiveInventory::Controllers::Items#modifyItemFields",
      "operationId": "modifyItemFields",
      "tags": ["inventory"],
      "produces": ["application/json"],
      "consumes": ["application/json"],
      "parameters": [
        {
          "name": "body",
          "in": "body",
          "required": true,
          "description": "Multiple items field modification data",
          "schema": {
            "type": "object",
            "required": ["items"],
            "properties": {
              "items": {
                "type": "array",
                "items": {
                  "type": "object",
                  "required": ["barcode", "fields"],
                  "properties": {
                    "barcode": {
                      "type": "string",
                      "description": "The barcode of the item to modify"
                    },
                    "fields": {
                      "type": "object",
                      "additionalProperties": {
                        "type": "string"
                      },
                      "description": "The fields to update with their new values"
                    }
                  }
                }
              }
            }
          }
        }
      ],
      "responses": {
        "200": {
          "description": "Items updated successfully",
          "schema": {
            "type": "object",
            "properties": {
              "results": {
                "type": "array",
                "items": {
                  "type": "object",
                  "properties": {
                    "barcode": {
                      "type": "string"
                    },
                    "status": {
                      "type": "integer"
                    },
                    "success": {
                      "type": "string"
                    },
                    "error": {
                      "type": "string"
                    }
                  }
                }
              }
            }
          }
        },
        "400": {
          "description": "Missing items data"
        }
      }
    }
  },
  "/item/checkin": {
    "post": {
      "x-mojo-to": "Com::InteractiveInventory::Controllers::Items#checkInItem",
      "operationId": "checkInItem",
      "tags": ["inventory"],
      "produces": ["application/json"],
      "consumes": ["application/json"],
      "parameters": [
        {
          "name": "body",
          "in": "body",
          "required": true,
          "description": "Item check-in data",
          "schema": {
            "type": "object",
            "required": ["barcode", "date"],
            "properties": {
              "barcode": {
                "type": "string",
                "description": "The barcode of the item to check in"
              },
              "date": {
                "type": "string",
                "description": "The date of check-in"
              }
            }
          }
        }
      ],
      "responses": {
        "200": {
          "description": "Item checked in successfully",
          "schema": {
            "type": "object",
            "properties": {
              "success": {
                "type": "string"
              }
            }
          }
        },
        "400": {
          "description": "Invalid input"
        },
        "404": {
          "description": "Item not found"
        },
        "500": {
          "description": "Internal server error"
        }
      }
    }
  },
  "/item/resolve_transit": {
    "post": {
      "x-mojo-to": "Com::InteractiveInventory::Controllers::Items#resolveTransit",
      "operationId": "resolveTransit",
      "tags": ["inventory"],
      "produces": ["application/json"],
      "consumes": ["application/json"],
      "parameters": [
        {
          "name": "body",
          "in": "body",
          "required": true,
          "description": "Transit resolution data",
          "schema": {
            "type": "object",
            "required": ["barcode"],
            "properties": {
              "barcode": {
                "type": "string",
                "description": "The barcode of the in-transit item"
              },
              "branchCode": {
                "type": "string",
                "description": "The target branch code (defaults to current branch if not provided)"
              }
            }
          }
        }
      ],
      "responses": {
        "200": {
          "description": "Transit resolved successfully",
          "schema": {
            "type": "object",
            "properties": {
              "status": {
                "type": "string"
              },
              "message": {
                "type": "string"
              }
            }
          }
        },
        "400": {
          "description": "Missing barcode or invalid request"
        },
        "404": {
          "description": "Item not found or not in transit"
        },
        "500": {
          "description": "Error resolving transit",
          "schema": {
            "type": "object",
            "properties": {
              "error": {
                "type": "string"
              }
            }
          }
        }
      }
    }
  },
  "/item/renew": {
    "post": {
      "x-mojo-to": "Com::InteractiveInventory::Controllers::Items#renewItem",
      "operationId": "renewItem",
      "tags": ["inventory"],
      "produces": ["application/json"],
      "consumes": ["application/json"],
      "parameters": [
        {
          "name": "body",
          "in": "body",
          "required": true,
          "description": "Item renewal data",
          "schema": {
            "type": "object",
            "required": ["barcode"],
            "properties": {
              "barcode": {
                "type": "string",
                "description": "The barcode of the item to renew"
              },
              "seen": {
                "type": "boolean",
                "description": "Whether the item was seen during renewal (defaults to true)"
              }
            }
          }
        }
      ],
      "responses": {
        "200": {
          "description": "Item renewed successfully",
          "schema": {
            "type": "object",
            "properties": {
              "success": {
                "type": "string"
              },
              "checkout_id": {
                "type": "integer"
              },
              "new_due_date": {
                "type": "string",
                "format": "date-time"
              },
              "renewals_count": {
                "type": "integer"
              }
            }
          }
        },
        "400": {
          "description": "Missing barcode or invalid request"
        },
        "403": {
          "description": "Cannot renew checkout"
        },
        "404": {
          "description": "Item not found or not checked out"
        },
        "500": {
          "description": "Error renewing item",
          "schema": {
            "type": "object",
            "properties": {
              "error": {
                "type": "string"
              }
            }
          }
        }
      }
    }
  },
  "/item/shelfbrowser/{itemnumber}": {
    "get": {
      "x-mojo-to": "Com::InteractiveInventory::Controllers::Items#shelfBrowser",
      "operationId": "shelfBrowser",
      "tags": ["inventory"],
      "produces": ["application/json"],
      "parameters": [
        {
          "name": "itemnumber",
          "in": "path",
          "required": true,
          "type": "integer",
          "description": "The itemnumber of the item to browse around"
        },
        {
          "name": "num_each_side",
          "in": "query",
          "required": false,
          "type": "integer",
          "default": 5,
          "description": "Number of items to return on each side of the current item"
        },
        {
          "name": "homebranch",
          "in": "query",
          "required": false,
          "type": "string",
          "description": "Filter by home library branch code"
        },
        {
          "name": "location",
          "in": "query",
          "required": false,
          "type": "string",
          "description": "Filter by shelving location code"
        },
        {
          "name": "ccode",
          "in": "query",
          "required": false,
          "type": "string",
          "description": "Filter by collection code"
        }
      ],
      "responses": {
        "200": {
          "description": "Nearby items retrieved successfully",
          "schema": {
            "type": "object",
            "additionalProperties": true
          }
        },
        "400": {
          "description": "Missing itemnumber parameter"
        },
        "404": {
          "description": "Item not found"
        },
        "500": {
          "description": "Error fetching nearby items",
          "schema": {
            "type": "object",
            "properties": {
              "error": {
                "type": "string"
              }
            }
          }
        }
      }
    }
  }
}
